{
  "name": "dam-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./.eslintrc.json --ignore-path ./.eslintignore ./*.js",
    "start-local": "npm run lint && PORT=8087 NODE_ENV=development nodemon index.js --exec babel-node",
    "win-start-local": "npm run lint && set PORT=8087&& set NODE_ENV=development&& nodemon index.js --exec babel-node",
    "start-mock-api": "cd ./swagger-gen/mock-api-server && npm --registry=https://registry.npm.taobao.org install && npm start",
    "start": "PORT=8088 NODE_ENV=production babel-node index.js",
    "start-test": "NODE_ENV=test babel-node index.js",
    "start-develop": "NODE_ENV=development babel-node index.js",
    "win-start": "set PORT=8088 && babel-node index.js"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "~1.16.0",
    "compression": "^1.7.0",
    "connect-redis": "^3.2.0",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "dam-web-client": "git+http://gitlab.vcg.com/dam/dam_web_client.git#develop",
    "debug": "~2.6.0",
    "ejs": "~2.5.5",
    "express": "~4.14.1",
    "express-http-proxy": "^1.0.3",
    "express-session": "^1.15.2",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.18.1",
    "mongoose": "^4.9.7",
    "morgan": "~1.7.0",
    "mysql": "^2.13.0",
    "oauth2orize": "^1.8.0",
    "orm": "^3.2.3",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "passport-oauth2-client-password": "^0.1.2",
    "querystring": "^0.2.0",
    "raw-body": "^2.2.0",
    "redis": "^2.7.1",
    "serve-favicon": "~2.3.2",
    "sprintf": "^0.1.5",
    "superagent": "^3.5.2",
    "svg-captcha": "^1.3.4",
    "uuid": "^3.0.1",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6",
    "yamljs": "^0.2.10"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "^3.19.0",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2"
  }
